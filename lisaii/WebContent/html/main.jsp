        let drop = $(this).children("ul.sub");
        // 2级列表基于当前状态收缩或展开
        drop.slideToggle()
        // 其他2级列表收缩
        $("ul.sub").not(drop).slideUp();
        // 阻止点击1级导航的跳转
        return false;
      });
    });
